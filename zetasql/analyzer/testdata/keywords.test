# ROW is an unreserved keyword, and can be used as an alias, column name,
# etc.
select row
from (select 1 as row);
--
QueryStmt
+-output_column_list=
| +-$subquery1.row#1 AS "row" [INT64]
+-query=
  +-ProjectScan
    +-column_list=[$subquery1.row#1]
    +-input_scan=
      +-ProjectScan
        +-column_list=[$subquery1.row#1]
        +-expr_list=
        | +-row#1 := Literal(type=INT64, value=1)
        +-input_scan=
          +-SingleRowScan

SELECT ARRAY<"zetasql_test__.MessageWithMapField.Uint64StringMapEntry">[
    NEW "zetasql_test__.MessageWithMapField.Uint64StringMapEntry"(
      1 AS key,
      "hello there" AS value
    ),
    NULL
  ][SAFE_KEY(1)]

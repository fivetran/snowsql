[name=bit_or_one_preceding_one_following]
SELECT t.row_id, bit_or_bq(t.int_val)
OVER(ORDER BY row_id ROWS BETWEEN 1 PRECEDING AND 1 FOLLOWING)
FROM TestTable t
ORDER BY row_id

ARRAY<STRUCT<ARRAY<>,
             ARRAY<>,
             ARRAY<>,
             ARRAY<>,
             ARRAY<>,
             ARRAY<>,
             ARRAY<>,
             STRING,
             STRING,
             STRING,
             STRING>>
[{ARRAY<INT64>(NULL),
  ARRAY<INT64>[unknown order:NULL, NULL],
  ARRAY<INT64>[unknown order:NULL, NULL, 1, 1],
  ARRAY<STRING>(NULL),
  ARRAY<STRING>[NULL],
  ARRAY<STRING>[unknown order:NULL, "abc"],
